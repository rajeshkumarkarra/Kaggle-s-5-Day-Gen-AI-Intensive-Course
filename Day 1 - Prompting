   "cell_type": "code",
   "execution_count": 4,
   "id": "c3cc8def",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-04-01T15:43:33.734029Z",
     "iopub.status.busy": "2025-04-01T15:43:33.733494Z",
     "iopub.status.idle": "2025-04-01T15:43:33.915402Z",
     "shell.execute_reply": "2025-04-01T15:43:33.914521Z"
    },
    "papermill": {
     "duration": 0.19841,
     "end_time": "2025-04-01T15:43:33.917814",
     "exception": false,
     "start_time": "2025-04-01T15:43:33.719404",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "from google.api_core import retry\n",
    "\n",
    "\n",
    "is_retriable = lambda e: (isinstance(e, genai.errors.APIError) and e.code in {429, 503})\n",
    "\n",
    "genai.models.Models.generate_content = retry.Retry(\n",
    "    predicate=is_retriable)(genai.models.Models.generate_content)"
   ]
  },
