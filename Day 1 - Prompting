   "cell_type": "code",
   "execution_count": 30,
   "id": "e495d3d8",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-04-01T15:06:03.364008Z",
     "iopub.status.busy": "2025-04-01T15:06:03.363213Z",
